.pos 0
init:
    irmovl Stack, %esp
    irmovl Stack, %ebp
    call Main
    halt

.align 4
array:
    .long 0xd
    .long 0xc0
    .long 0xb00
    .long 0xa000

Main:
    pushl %ebp
    rrmovl %esp, %ebp

    ; Do something
    @loop:

    rrmovl %ebp, %esp
    popl %ebp
    ret

    .long 0xFFFFFF

.pos 0x100
Stack: